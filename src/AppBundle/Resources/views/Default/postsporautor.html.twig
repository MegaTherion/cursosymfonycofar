{% extends '::base.html.twig' %}

{% block body %}

	{% for flashMessage in app.session.flashbag.get('notice') %}
	    <div class="flash-notice">
	        {{ flashMessage }}
	    </div>
	{% endfor %}

	<ul>
    {% for post in posts %}
		<li>{{ post.titulo }}</li>        
    {% endfor %}
	</ul>
{% endblock %}